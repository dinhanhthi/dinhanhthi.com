  "scripts": {
    "dev": "next dev -p 3004",
    "build": "next build",
    "postinstall": "pnpm rebuild sharp",
    "start": "next start -p 3004",
    "lint": "eslint .",
    "check": "pnpm run prettier && pnpm run build",
    "prettier": "prettier --write \"**/*.{tsx,ts,css,scss}\"",
    "clean": "rimraf .next && rimraf .vercel && rimraf .netlify",
    "clean-build": "pnpm run clean && pnpm run build",
    "reinstall": "pnpm run clean && rm -rf node_modules 2>/dev/null && rm pnpm-lock.yaml 2>/dev/null && pnpm install",
    "warm-cache": "npx tsx -r dotenv/config ./scripts/warm-cache.ts dotenv_config_path=.env.production.local",
    "clear-cache": "npx tsx -r dotenv/config ./scripts/clear-cache.ts dotenv_config_path=.env.production.local"
  },
